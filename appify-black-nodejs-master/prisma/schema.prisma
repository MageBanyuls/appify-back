generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model agendamiento_proyecto {
  id       Int       @id @default(autoincrement())
  proyecto String    @db.VarChar(100)
  user     String    @db.VarChar(100)
  duracion DateTime? @db.Time(0)
  fecha    DateTime? @db.Date

  @@index([proyecto], map: "proyectos")
  @@index([user], map: "subusuarios")
}

model bancos {
  id     String @id @db.VarChar(100)
  user   String @db.VarChar(100)
  nombre String @db.VarChar(100)
  codigo String @db.VarChar(100)

  @@index([user], map: "usuarios")
}

model categoria_cuenta {
  id         String  @id @db.VarChar(100)
  user       String  @db.VarChar(100)
  nombre     String  @db.VarChar(75)
  codigo     String  @db.VarChar(75)
  activo     Boolean
  pasivo     Boolean
  patrimonio Boolean
  resultado  Boolean
  orden      Boolean

  @@index([user], map: "usuarios")
}

model clientes {
  id                      String    @id @db.VarChar(100)
  user                    String    @db.VarChar(100)
  razon_social            String    @db.VarChar(200)
  rut                     String    @db.VarChar(30)
  giro                    String    @db.VarChar(200)
  direccion               String    @db.VarChar(200)
  comuna                  String    @db.VarChar(200)
  ciudad                  String    @db.VarChar(200)
  suscripcion_a_encuestas Boolean?
  activo                  Boolean?
  condicion_de_pago       String    @db.VarChar(100)
  linea_de_credito        Float?    @db.Float
  exension_de_impuestos   Boolean?
  notas                   String?   @db.Text
  pie_de_pagina           String?   @db.Text
  fecha_creado            DateTime? @default(now()) @db.Timestamp(0)

  @@index([user], map: "usuarios")
  @@index([condicion_de_pago], map: "condicion_pago")
}

model cobros {
  id                    String   @id @db.VarChar(100)
  user                  String   @db.VarChar(100)
  cuenta_banco          String   @db.VarChar(100)
  clienteId             String   @db.VarChar(100)
  pagado                Boolean  @default(false)
  cuenta_origen_destino String   @db.VarChar(100)
  fecha                 DateTime @db.Date
  movimiento_monto      Decimal? @db.Decimal(30, 2)
  tipo                  String   @db.VarChar(50)
  tipo_n                String   @db.VarChar(100)
  glosa                 String?  @db.Text

  @@index([user], map: "usuarios")
  @@index([clienteId], map: "clientes")
  @@index([cuenta_banco], map: "cuentas_banco")
}

model condiciones_condicion_pago {
  id          String @id @db.VarChar(100)
  idCondicion String @db.VarChar(100)
  porcentaje  Float  @db.Float
  numero_dias Int

  @@index([idCondicion], map: "condicion_pago")
}

model consultas {
  id             Int     @id @default(autoincrement())
  idVendedor     String  @db.VarChar(100)
  nombre         String  @db.VarChar(200)
  email          String  @db.VarChar(200)
  celular        String  @db.VarChar(30)
  nombre_empresa String  @db.VarChar(50)
  consulta       String? @db.Text
  archivo        String? @db.VarChar(200)
  estado         Boolean

  @@index([idVendedor], map: "subusuarios")
}

model contactos_por_cliente {
  id         String  @id @db.VarChar(100)
  cliente    String  @db.VarChar(100)
  nombre     String? @db.VarChar(200)
  email      String? @db.VarChar(200)
  celular    String? @db.VarChar(30)
  telefono   String? @db.VarChar(30)
  vendedorId String  @db.VarChar(100)

  @@index([cliente], map: "clientes")
  @@index([vendedorId], map: "subusuarios")
}

model costos_por_proyecto {
  id       String    @id @db.VarChar(100)
  proyecto String    @db.VarChar(100)
  nombre   String?   @db.VarChar(200)
  fecha    DateTime? @db.Date
  cantidad Int?
  costo    Float?    @db.Float
  total    Int?
  notas    String?   @db.Text

  @@index([proyecto], map: "proyectos")
}

model cuenta_tipo_documento {
  id      String  @id @db.VarChar(100)
  user    String  @db.VarChar(100)
  nombre  String  @db.VarChar(100)
  credito Boolean
  contado Boolean

  @@index([user], map: "usuarios")
}

model cuentas_banco {
  id                    String  @id @db.VarChar(100)
