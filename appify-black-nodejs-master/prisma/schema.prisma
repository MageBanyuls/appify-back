  moneda_secundaria     Boolean
  estado                Boolean
  predeterminada_pagos  Boolean
  predeterminada_cobros Boolean
  documentos_permitidos Boolean

  @@index([user], map: "usuarios")
  @@index([bancoId], map: "bancos")
}

model direccion_de_prestacion_proyecto {
  id         String  @id @db.VarChar(100)
