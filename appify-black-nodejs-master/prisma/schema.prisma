  nombre                String  @db.VarChar(100)
  bancoId               String  @db.VarChar(100)
  tipo_cuenta           String  @db.VarChar(100)
  nro_cuenta_corriente  String  @db.VarChar(100)
  codigo                String  @db.VarChar(100)
