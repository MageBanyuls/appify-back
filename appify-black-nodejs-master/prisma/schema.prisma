  direccion  String? @db.VarChar(100)
  comuna     String? @db.VarChar(100)
  ciudad     String? @db.VarChar(100)
  duracion   String? @db.VarChar(100)

  @@index([proyecto], map: "proyectos")
  @@index([punto], map: "puntos_de_despacho_por_cliente")
}

model documento_compra {
